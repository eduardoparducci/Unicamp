# EDUARDO PARDUCCI RA:170272

# VARIAVEIS INICIAIS GX=35000
100 0000000009
101 00000088B8
102 0000000001

# INICIO DO PROGRAMA
LOAD M(0X101) #AC:= Y
RSH           #AC:= Y/2

STOR M(0X103) #M(0X103):= K
LOAD M(0X101) #AC:= Y

DIV M(0X103)  #MQ:= Y/K
LOAD MQ       #AC:= MQ

ADD M(0X103)  #AC:= AC+K
RSH           #AC:= AC/2

STOR M(0X103) #M(0X103):= K
LOAD M(0X100) #AC:= N

SUB M(0X102)  #AC:= AC-1
STOR M(0X100) #M(0X100):= AC

JUMP+M(0X001,20:39)